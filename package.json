{
  "name": "gitlab-extra",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "test": "vitest",
    "test:ci": "vitest run --maxWorkers 1 --minWorkers 1",
    "test:acceptance": "cd test/acceptance && pnpm test",
    "test:acceptance:ci": "cd test/acceptance && pnpm test:ci",
    "lint": "pnpm /^lint:.*$/",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .js,.ts,.cjs,.mts,.vue .",
    "lint:ts": "nuxt typecheck",
    "format": "prettier --ignore-unknown --check ."
  },
  "dependencies": {
    "@auth/core": "^0.30.0",
    "@hebilicious/authjs-nuxt": "^0.3.5",
    "@mdi/font": "^7.4.47",
    "@nuxt/kit": "^3.11.2",
    "@vuetify/loader-shared": "^2.0.3",
    "@vueuse/core": "^10.9.0",
    "nuxt": "^3.11.2",
    "sass": "^1.75.0",
    "vite-plugin-vuetify": "^2.0.3",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vuetify": "^3.5.17"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.2.2",
    "@nuxt/eslint-config": "^0.3.10",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.0.3",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.25.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "msw": "^2.2.14",
    "prettier": "^3.2.5",
    "resize-observer-polyfill": "^1.5.1",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vitest": "^1.5.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,ts,cjs,mts,vue}": "eslint --ignore-path .gitignore --fix"
  },
  "packageManager": "pnpm@9.0.6"
}
